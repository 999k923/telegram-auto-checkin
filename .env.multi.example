# ============================================
# Telegram 自动签到 - 多目标配置示例
# ============================================

# Telegram API 配置
API_ID=12345678
API_HASH=abcdef1234567890abcdef1234567890
PHONE_NUMBER=+8613800138000

# ============================================
# 多目标签到配置
# ============================================

# 示例：机器人（按钮签到）+ 群组（命令签到）
CHECKIN_TARGETS=[{"name":"@okemby_bot","target":"@okemby_bot","command":"/start","button_text":"签到"},{"name":"Cloud Cat Group","target":"Cloud Cat Group","command":"/checkin","button_text":""}]

# 其他示例（取消注释使用）：

# 示例1：两个机器人都用按钮
# CHECKIN_TARGETS=[{"name":"机器人A","target":"@bot_a","command":"/start","button_text":"签到"},{"name":"机器人B","target":"@bot_b","command":"/menu","button_text":"打卡"}]

# 示例2：两个群组都用命令
# CHECKIN_TARGETS=[{"name":"群组1","target":"Group 1","command":"/checkin","button_text":""},{"name":"群组2","target":"Group 2","command":"/sign","button_text":""}]

# 示例3：三个目标混合
# CHECKIN_TARGETS=[{"name":"机器人","target":"@mybot","command":"/start","button_text":"签到"},{"name":"群组A","target":"Group A","command":"/checkin","button_text":""},{"name":"群组B","target":"Group B","command":"/daily","button_text":"每日打卡"}]

# ============================================
# 时间和延迟配置
# ============================================

# 签到时间（24小时制）
CHECKIN_HOUR=9
CHECKIN_MINUTE=0

# 时区
TIMEZONE=Asia/Shanghai

# 随机延迟（秒）
RANDOM_DELAY_MIN=0
RANDOM_DELAY_MAX=300
