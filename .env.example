RANDOM_DELAY_MAX=300

# 签到目标配置（多目标支持）
# 格式：JSON 数组
# 示例：
# CHECKIN_TARGETS=[{"name":"示例群组","target":"Cloud Cat Group","command":"","button_text":"签到"},{"name":"示例机器人","target":"@some_bot","command":"/checkin","button_text":""}]
